{
    "name": "everyworkflow/scope-bundle",
    "description": "Scope bundle for every workflow project.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "EveryWorkflow",
            "email": "everyworkflow@gmail.com"
        }
    ],
    "require": {
        "everyworkflow/core-bundle": "dev-main",
        "everyworkflow/mongo-bundle": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "EveryWorkflow\\ScopeBundle\\": "./"
        },
        "exclude-from-classmap": [
            "Resources",
            "Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryWorkflow\\ScopeBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
